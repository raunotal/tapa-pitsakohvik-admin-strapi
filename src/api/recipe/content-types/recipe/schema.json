{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "DurationInMinutes": {
      "type": "integer",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "Comments": {
      "type": "text"
    },
    "stages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stage.stage",
      "mappedBy": "recipe"
    },
    "recipe_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recipe-product.recipe-product",
      "mappedBy": "recipe"
    },
    "Photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product",
      "inversedBy": "recipe"
    },
    "recipe_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-group.product-group",
      "inversedBy": "recipes"
    }
  }
}
